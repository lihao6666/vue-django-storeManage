.circle {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
  position: fixed;
  z-index: 999;
}
.animation0::before, .animation0::after {
  position: absolute;
  content: '';
  display: block;
  width: 140%;
  height: 100%;
  left: -20%;
  z-index: -1000;
  -webkit-transition: all ease-in-out 0.5s;
  transition: all ease-in-out 0.5s;
  background-repeat: no-repeat;
}
